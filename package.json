{
  "name": "frame-by-frame-netlify",
  "version": "1.0.0",
  "description": "Frame By Frame - Image Mosaic Generator",
  "main": "index.js",
  "scripts": {
    "dev": "netlify dev",
    "clean": "rm -rf dist",
    "prebuild": "npm install sharp",
    "build": "npm ci && npm run clean && webpack --mode production && cp -r public/* dist/ && cp index.html dist/",
    "start": "webpack serve --mode development"
  },
  "keywords": [
    "mosaic",
    "image-processing",
    "netlify",
    "supabase"
  ],
  "author": "Callum O'Connor, Yipeng Zhang, Yiran Dai",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.540.0",
    "@supabase/supabase-js": "^2.39.3",
    "aws-sdk": "^2.1473.0",
    "canvas": "^2.11.2",
    "node-fetch": "^2.7.0",
    "parse-multipart-data": "^1.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.32.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.10.0",
    "html-webpack-plugin": "^5.6.0",
    "netlify-cli": "^17.15.1",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
} 